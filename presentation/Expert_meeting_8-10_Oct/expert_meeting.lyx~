\end_layout

\begin_layout Plain Layout

	
\backslash
begin{itemize}
\end_layout

\begin_layout Plain Layout

		
\backslash
item All three NNs in the cascade have the same topology
\end_layout

\begin_layout Plain Layout

			
\backslash
begin{itemize}
\end_layout

\begin_layout Plain Layout

				
\backslash
item input layer of size 15 
\end_layout

\begin_layout Plain Layout

				
\backslash
item hidden layer of size 6 
\end_layout

\begin_layout Plain Layout

				
\backslash
item output layer of size 2 
\end_layout

\begin_layout Plain Layout

		
\backslash
end{itemize}
\end_layout

\begin_layout Plain Layout

		
\backslash
item All layers are dense with sigmoid activation 
\end_layout

\begin_layout Plain Layout

		
\backslash
item Total 110 tunable weights
\end_layout

\begin_layout Plain Layout

	
\backslash
end{itemize}
\end_layout

\begin_layout Plain Layout

}
